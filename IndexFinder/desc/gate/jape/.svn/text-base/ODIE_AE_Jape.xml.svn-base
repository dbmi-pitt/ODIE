<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>edu.pitt.dbmi.odie.uima.gate.jape.ae.ODIE_JapeTAE</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>ODIE_AE_Jape</name>
    <description>Text Annotation Engine the delegates it's behavior to
a GATE JAPE Processing Resource.   Annotations from the Common Annotation Structure are converted to gate.Annotation objects using introspection on accessors (aka getters) and mutators (aka setters).</description>
    <version>1.0</version>
    <vendor>University of Pittsburgh DBMI ODIE Project</vendor>
    <configurationParameters>
      <configurationParameter>
        <name>odie.gate.script.key.key</name>
        <description>Internal Key that indicates the resource for the gate script.</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>odie.gate.script.key.key</name>
        <value>
          <string>ODIE_Jape_Processing_Resource</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import location="../../cTAKES/core/type/TypeSystem.xml"/>
        <import location="../type/ODIE_GateTypeSystemDescriptor.xml"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <externalResourceDependencies>
    <externalResourceDependency>
      <key>ODIE_Gate_Factory</key>
      <description/>
      <interfaceName>edu.pitt.dbmi.odie.uima.gate.resource.ODIE_GateResource</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
    <externalResourceDependency>
      <key>ODIE_Jape_Processing_Resource</key>
      <description/>
      <interfaceName>edu.pitt.dbmi.odie.uima.gate.resource.ODIE_GateProcessingResourceUrlResource</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
  </externalResourceDependencies>
  <resourceManagerConfiguration>
    <externalResources>
      <externalResource>
        <name>odie.gate.factory</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:gate/odiehome</fileUrl>
        </fileResourceSpecifier>
        <implementationName>edu.pitt.dbmi.odie.uima.gate.resource.ODIE_GateResourceImpl</implementationName>
      </externalResource>
      <externalResource>
        <name>odie.jape.pr</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:gate/odiehome/jape/ODIE_NER_Discoverer_Pre_Noun_Chunker.jape</fileUrl>
        </fileResourceSpecifier>
        <implementationName>edu.pitt.dbmi.odie.uima.gate.resource.ODIE_GateProcessingResourceUrlResourceImpl</implementationName>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>ODIE_Gate_Factory</key>
        <resourceName>odie.gate.factory</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>ODIE_Jape_Processing_Resource</key>
        <resourceName>odie.jape.pr</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  </resourceManagerConfiguration>
</analysisEngineDescription>
