<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="ODIE_AAE_LspOntologyEnricher">
      <import location="../lsp/ODIE_AAE_LspOntologyEnricher.xml"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="ODIE_AAE_INDEX_FINDER_PROPOSING">
      <import location="../indexfinder/ODIE_AAE_INDEX_FINDER_PROPOSING.xml"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>ODIE Ontology Enrichment Engine (LSP)</name>
    <description>Runs a two pass Indexfinder NER engine, one each for target ontology and one for new concept proposals.

Lexical syntactic patterns(LSP) are used to generate a list of suggestions for new concepts.</description>
    <version>1.0</version>
    <vendor>DBMI, University of Pittsburgh</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>analysis.db.url</name>
        <description>Database url</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>ODIE_AAE_LspOntologyEnricher/db.url</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>is.contiguous.prop</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>ODIE_AAE_INDEX_FINDER_PROPOSING/odie.is.contiguous1</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>is.overlapping.prop</name>
        <description>When true return overlapping annotations</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>ODIE_AAE_INDEX_FINDER_PROPOSING/odie.is.overlapping1</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>is.overlapping.if</name>
        <description>When true return overlapping annotations</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>ODIE_AAE_INDEX_FINDER_PROPOSING/odie.is.overlapping</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>is.contiguous.if</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>ODIE_AAE_INDEX_FINDER_PROPOSING/odie.is.contiguous</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>db.driver</name>
        <description>Db driver</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>ODIE_AAE_LspOntologyEnricher/db.driver</parameter>
          <parameter>ODIE_AAE_INDEX_FINDER_PROPOSING/ODIE_IndexFinderTAE.odie.db.driver</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>db.username</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>ODIE_AAE_LspOntologyEnricher/db.username</parameter>
          <parameter>ODIE_AAE_INDEX_FINDER_PROPOSING/ODIE_IndexFinderTAE.odie.db.user.name</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>db.password</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>ODIE_AAE_LspOntologyEnricher/db.password</parameter>
          <parameter>ODIE_AAE_INDEX_FINDER_PROPOSING/ODIE_IndexFinderTAE.odie.db.user.password</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>if.db.url</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>ODIE_AAE_INDEX_FINDER_PROPOSING/ODIE_IndexFinderTAE.odie.db.url</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>proposal.if.db.url</name>
        <description>Jdbc url</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>ODIE_AAE_INDEX_FINDER_PROPOSING/odie.db.url</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>is.contiguous.prop</name>
        <value>
          <string>true</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>is.overlapping.prop</name>
        <value>
          <string>false</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>is.overlapping.if</name>
        <value>
          <string>false</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>is.contiguous.if</name>
        <value>
          <string>true</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>analysis.db.url</name>
        <value>
          <string>jdbc:mysql://localhost:3306/odie_1013</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>db.driver</name>
        <value>
          <string>org.gjt.mm.mysql.Driver</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>db.password</name>
        <value>
          <string>password</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>if.db.url</name>
        <value>
          <string>jdbc:mysql://localhost:3306/od_lex_ifbuild</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>proposal.if.db.url</name>
        <value>
          <string>jdbc:mysql://localhost:3306/od_lex_ifbuild</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>db.username</name>
        <value>
          <string>username</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>ODIE_AAE_INDEX_FINDER_PROPOSING</node>
        <node>ODIE_AAE_LspOntologyEnricher</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
