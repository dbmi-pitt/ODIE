<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>edu.pitt.dbmi.odie.uima.lsp.ae.ODIE_LspTAE</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>ODIE_LspTAE</name>
    <description>UIMA Wrapper for LSP Patterns Concept Discovery</description>
    <version>1.0</version>
    <vendor>University of Pittsburgh ODIE Project</vendor>
    <configurationParameters>
      <configurationParameter>
        <name>odie.db.driver</name>
        <description>RDBMS driver</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>odie.db.url</name>
        <description>Jdbc url</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>odie.db.user.name</name>
        <description>User name</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>odie.db.user.password</name>
        <description>User password</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>odie.db.driver</name>
        <value>
          <string>org.gjt.mm.mysql.Driver</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>odie.db.url</name>
        <value>
          <string>jdbc:mysql://localhost:3306/lsp_analysis2</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>odie.db.user.name</name>
        <value>
          <string>caties</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>odie.db.user.password</name>
        <value>
          <string>caties</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import location="../cTAKES/core/type/NamedEntity.xml"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.type.NamedEntity</type>
        </inputs>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
