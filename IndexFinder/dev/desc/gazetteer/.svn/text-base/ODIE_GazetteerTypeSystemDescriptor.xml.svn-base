<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>ODIE_GazetteerTypeSystemDescriptor</name>
  <description>Duplicates the functionality of the GATE Gazetteer Processing Resource.</description>
  <version>1.0</version>
  <vendor>University of Pittsburgh DBMI ODIE project</vendor>
  <types>
    <typeDescription>
      <name>edu.pitt.dbmi.odie.uima.gazetteer.type.Lookup</name>
      <description>UIMA implementation equivalent to GATE's Lookup annotation.</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>majorType</name>
          <description>Major category of this type.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>minorType</name>
          <description>Minor category of this type.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
